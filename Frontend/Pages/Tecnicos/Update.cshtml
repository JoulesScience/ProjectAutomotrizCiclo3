@page
@model MyApp.Namespace.UpdateModel2
@{
}
<div class= "justify-conten-center text-left">
    <h1>Modificar técnico</h1>
</div>
<br>
<div class="row"></div>
    <div class="col-md-4">
        <form method="post">                 
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Tecnico.Especialidad" class="Control-Label"></label>
                <input asp-for="Tecnico.Especialidad" class="form-control"/>
                <span asp-validation-for="Tecnico.Especialidad" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Tecnico.Identificaion_id" class="Control-Label"></label>
                <input asp-for="Tecnico.Identificaion_id" class="form-control"/>
                <span asp-validation-for="Tecnico.Identificaion_id" class="text-danger"></span>
            </div>
            <input type="hidden" asp-for="Tecnico.Tecnico_id"/>
            <input type="hidden" asp-for="Tecnico"/>
            <div class="form-group">
                <input type="submit" value="modificar tecnico" class="btn btn-primary" asp-page="Update/OnPost"/>
            </div>
            <br>
            <div>
                <a asp-page="./List" class="btn btn-primary">Volver</a>
            </div>
        </form>
    </div>
</div>





